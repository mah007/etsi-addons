<odoo>
    <data>
        <record id="stock_picking_inherited_form_view" model="ir.ui.view">
            <field name="name">stock.picking.inherited.form.view</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">

                <field name="product_qty" position="attributes">
                    <attribute name="string">quantity order</attribute>
                </field>

                <field name="qty_done" position="attributes">
                    <attribute name="string">quantity receive</attribute>
                </field>

            </field>
        </record>
    </data>
</odoo>