<odoo>
    <data>

 <!--Vessel>Configuation>Vessel> -->

       <menuitem id="vessel_cfg_menu"
          name="Vessel"
          parent="vessel_config_menu"/>

<!--Vessel>Configuation>Vessel>Type -->

        <record id="vessel_type_search_view" model="ir.ui.view">
            <field name="name">vessel.type.search.view</field>
            <field name="model">vessel.type</field>
            <field name="arch" type="xml">
                <search string="Vessel Type">
                    <field name="name"/>
                </search>
            </field>
        </record>

        <record id="vessel_type_tree_view" model="ir.ui.view">
            <field name="name">vessel.type.tree.view</field>
            <field name="model">vessel.type</field>
            <field name="arch" type="xml">
                <tree string="Vessel Type">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="vessel_type_form_view" model="ir.ui.view">
            <field name="name">vessel.type.form.view</field>
            <field name="model">vessel.type</field>
            <field name="arch" type="xml">
                <form string="Vessel Type">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="desc"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="vessel_type_action">
            <field name="name">Vessel Type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vessel.type</field>
            <field name="view_type">form</field>
            <field name="view_mode">search,tree,form</field>
        </record>

        <menuitem id="vessel_type_menu"
                  name="Type"
                  action="vessel_type_action"
                  parent="vessel_cfg_menu"/>

