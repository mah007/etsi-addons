<odoo>
    <data>

        <record model="ir.ui.view" id="wh_config_tree_view">
            <field name="name">wh.config.tree.view</field>
            <field name="model">warehouse.config</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="wh_address"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="wh_config_form_view">
            <field name="name">wh.config.form.view</field>
            <field name="model">warehouse.config</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="wh_address"/>
                        </group>
                        <field name="assigned_asset_ids" widget="many2many">
                            <tree>
                                <field name="asset_id"/>
                                <field name="asset_condition"/>
                                <field name="asset_distribution_date"/>
                                <field name="asset_quantity"/>
                            </tree>
                        </field>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="wh_config_search_view">
            <field name="name">wh.config.search.view</field>
            <field name="model">warehouse.config</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                    <field name="wh_address"/>
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="wh_config_action">
          <field name="name">Warehouse</field>
          <field name="res_model">warehouse.config</field>
          <field name="view_type">form</field>
          <field name="view_mode">tree,form,search</field>
            <field name="help" type="html">
                <p class="oe_view_nocomment_create">Create Warehouse</p>
            </field>
        </record>

        <menuitem
            id = "wh_config_menu"
            name = "Warehouse"
            parent = "asset_config_main_menu"
            action = "wh_config_action"
            />

    </data>
</odoo>